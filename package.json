{
  "name": "data-tab-board",
  "version": "0.0.1",
  "description": "A data chart board with tabs and expot",
  "main": "index.tsx",
  "scripts": {
    "build": "dumi build",
    "compile": "dumi build && lessc src/index.less src/index.css",
    "coverage": "dumi test --coverage",
    "lint": "eslint src/ examples/ --ext .tsx,.ts,.jsx,.js",
    "now-build": "npm run build",
    "prepublishOnly": "npm run compile && np --no-cleanup --yolo --no-publish",
    "start": "cross-env NODE_ENV=development dumi dev",
    "test": "dumi test",
    "gh-pages": "npm run build && gh-pages -d dist",
    "update:deps": "yarn upgrade-interactive --latest"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ybzjsxh/data-tab-board.git"
  },
  "keywords": [
    "data",
    "visualize",
    "react",
    "tab",
    "excel"
  ],
  "author": "musiq841",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ybzjsxh/data-tab-board/issues"
  },
  "homepage": "https://github.com/ybzjsxh/data-tab-board#readme",
  "dependencies": {
    "@ant-design/charts": "^0.9.13",
    "antd": "^4.6.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "chalk": "^4.1.0",
    "cross-env": "^7.0.2",
    "dumi": "^1.0.34",
    "eslint": "^7.8.1",
    "eslint-plugin-react-hooks": "^4.1.0",
    "gh-pages": "^3.1.0",
    "less": "^3.12.2",
    "lint-staged": "^10.3.0",
    "np": "^6.5.0",
    "prettier": "^2.1.1",
    "typescript": "^4.0.2",
    "yorkie": "^2.0.0"
  },
  "peerDependencies": {
    "react": "^16.12.2",
    "react-dom": "^16.12.2"
  }
}
